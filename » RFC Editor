  .sub-menu li a { padding-left: 1em; }
  .sub-menu .sub-menu li a { padding-left: 1.5em; }
  #navmenu #searchform { float: right; margin: 0px; position: absolute; top: -45px; right: 15px; }
  #navmenu #searchform #s { width: 120px; }
  #headerimgwrap { top: 10% !important; }
  #headerimg { max-width: 100%; height: 50px; width: 255px; }
  .top, .sticky .top { position: static; background: none !important; box-shadow: none !important; }
  .top:hover { transform: none; }
  .entry, .sticky .entry { background: none !important; border: none !important; box-shadow: none !important; }
  .post { margin-bottom: 20px; }
  .post_title { margin-top: 10px; }
  .navigation { clear: both; }
  .navigation div { display: block; background-color: rgb(227, 227, 227); z-index: 10; box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 2px; font-size: 18px; border: 1px solid rgb(170, 170, 170); padding: 5px 8px; border-radius: 2px; }
  .scrolltable { width: 100%; overflow: scroll; }
  table.erratasearch th, table.erratasearch td, table.erratasearch tr { display: block; width: 240px; }
}
